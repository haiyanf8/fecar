.main_container {
  overflow: hidden;
  _zoom: 1;
}

.main {
  margin-top: 63px;
  margin-bottom: 87px;
}
.main-left {
  width: 418px;
  float: left;
}
.main-right {
  width: 750px;
  height: 635px;
  float: right;
  border: 1px solid #f0f0ef;
  border-radius: 8px;
}

.m-city {
  height: 100px;
  margin-bottom: 16px;
  border: 1px solid #dbdbdb;
  border-radius: 8px;
  cursor: pointer;
  overflow: hidden;
  -webkit-transition: all 0.5s ease 0s;
          transition: all 0.5s ease 0s;
  position: relative;
}
.m-city.last {
  margin-bottom: 0;
}
.m-city-tt {
  margin-top: 20px;
  margin-left: 25px;
  font-size: 20px;
  line-height: 26px;
}
.m-city-tt .before {
  width: 20px;
  height: 18px;
  margin-top: 4px;
  margin-right: 7px;
  background: url(../img/store/m-city-tt.png) center center no-repeat;
}
.m-city-tt .goto-map {
  width: 19px;
  height: 12px;
  float: right;
  margin-right: 20px;
  padding: 7px 5px;
  background: url(../img/store/m-city-goto.png) center center no-repeat;
}
.m-city-addr {
  padding-right: 0;
  margin-top: 7px;
  margin-left: 25px;
  font-size: 16px;
  line-height: 22px;
}
.m-city-addr.fztxt {
  height: 22px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.m-city-img {
  width: 216px;
  height: 160px;
  display: none;
  margin: 10px auto 0;
  text-align: center;
  overflow: hidden;
}
.m-city-img img {
  width: 100%;
  border-radius: 6px;
}
.m-city.current {
  height: 280px;
  border-color: #ff6b1d;
}
.m-city.current .m-city-img {
  display: block;
}
.m-city.current .m-city-tt .before {
  background-image: url(../img/store/m-city-tt--current.png);
}
.m-city.current .m-city-tt .goto-map {
  background-image: url(../img/store/m-city-goto--current.png);
}
.m-city.current .fztxt {
  height: auto;
}

.m-sh-tt {
  padding-left: 25px;
  margin: 8px 0;
  overflow: hidden;
}
.m-sh-tt .item {
  float: left;
  width: 68px;
  height: 24px;
  line-height: 24px;
  text-align: center;
}
.m-sh-tt .item.current {
  color: #ffffff;
  font-size: 12px;
  border-radius: 3px;
  background: #ff6b1d;
}
.m-sh-cont .item {
  display: none;
  clear: both;
}
.m-sh-cont .item p {
  letter-spacing: -1px;
}
.m-sh-cont .item.active {
  display: block;
}

.m-map {
  position: relative;
}
.m-map-fake {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  background: url(../img/store/m-map-fake.png) center center no-repeat;
}
.m-map-fake .point {
  position: absolute;
  width: 62px;
  height: 32px;
  background-position: center center;
  background-repeat: no-repeat;
  cursor: pointer;
}
.m-map-baidu {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 5;
}

.city-xiamen {
  margin: 25px auto 10px auto;
}

.city-shanghai {
  margin: 25px auto 10px auto;
}

