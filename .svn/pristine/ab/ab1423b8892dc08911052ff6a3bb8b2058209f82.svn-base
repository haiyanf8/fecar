

<div class="main js-ls-goto-map">
	<div class="main_container">

		<div class="main-left">
			<ul class="main-left-nav js-map-chosen">
				<volist name="store" id="vo">
					<li class="item js-item--chosen" data-city="{$vo.pinyin}">{$vo.city}</li>
				</volist>
			</ul>

			<ul class="main-left-content js-map-target">
				<volist name="store" id="vo" key="k">
					<if condition="$k == 0">
						<li class="item js-item--target active">
					<else />
						<li class="item js-item--target">
					</if>
						<img  class="img" src="{$vo.image}">
						<div class="txt">
							<p class="icon"></p>
							<p class="address"><span>地址：{$vo.address}</span></p>
						</div>
					</li>
				</volist>
			</ul>
		</div>

		<div class="main-right m-map">
			<div id="m-map-baidu" class="m-map-baidu js-map-baidu"></div>
		</div>
	</div>
</div>

