html {
  margin: 0;
  padding: 0;
  min-width: 1280px;
  font-size: 12px;
  background-color: #ffffff;
}

body {
  margin: 0;
  padding: 0;
  width: 100%;
  min-width: 1280px;
  color: #3c3c3c;
  font-family: Microsoft Yahei, Arial, sans-serif;
}

p {
  margin: 0;
  padding: 0;
}

ul, ol, li {
  list-style-type: none;
  margin: 0;
  padding: 0;
}

a {
  color: inherit;
  *color: #3c3c3c;
  text-decoration: none;
  border: none;
  outline: none;
}

h1, h2, h3, h4, h5, h6, strong, em {
  margin: 0;
  padding: 0;
  font-weight: normal;
  color: inherit;
  font-size: inherit;
}

input, button {
  font-family: Microsoft Yahei, Arial, sans-serif;
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0;
  line-height: normal;
}
input:focus, button:focus {
  outline: none;
}

img {
  border: 0;
}

i, .i {
  display: inline-block;
}

.pbl-msg-alert {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background-repeat: repeat;
  background-image: url("../img/pbl/tspr-50.png");
  display: none;
}
.pbl-msg-alert.active {
  display: block;
}
.pbl-msg-alert .wrap--content {
  width: 500px;
  position: absolute;
  top: 50%;
  left: 50%;
  overflow: hidden;
  margin-left: -250px;
  margin-top: -100px;
  background-color: #ffffff;
}
.pbl-msg-alert .wrap--content > .title {
  height: 49px;
  width: 80%;
  margin: 0 auto;
  text-align: center;
  font-size: 16px;
  line-height: 49px;
  border-bottom: 1px solid #dbdbdb;
}
.pbl-msg-alert .wrap--content > .content {
  width: 65%;
  overflow: hidden;
  margin: 9px auto;
  text-align: center;
  font-size: 14px;
  line-height: 21px;
}
.pbl-msg-alert .wrap--content > .content .strong {
  color: #ff6b1d;
}
.pbl-msg-alert .wrap--content > .content .strong-red {
  color: #fe461c;
}
.pbl-msg-alert .wrap--content > .content img {
  margin-top: 10px;
  width: 50%;
}
.pbl-msg-alert .wrap--content > .btn--yes {
  width: 100px;
  height: 30px;
  overflow: hidden;
  margin: 20px auto;
  font-size: 16px;
  text-align: center;
  line-height: 30px;
  color: #ffffff;
  background-color: #ff6b1d;
  border-radius: 3px;
  cursor: pointer;
  -webkit-user-select: none;
     -moz-user-select: none;
      -ms-user-select: none;
          user-select: none;
}
.pbl-msg-alert .wrap--content > .close {
  width: 15px;
  height: 15px;
  position: absolute;
  top: 10px;
  right: 10px;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("../img/pbl/close.png");
  cursor: pointer;
}

.pbl-success-alert {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
